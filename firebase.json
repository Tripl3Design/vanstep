{
  "hosting": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors https://www.tripledesign.nl https://tripledesign.nl https://www.vanstep.nl https://vanstep.nl http://127.0.0.1:5000 https://vanstep-vanstep.web.app"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Permissions-Policy",
            "value": "xr-spatial-tracking=(self)"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          }
        ]
      }
    ],
    "target": "vanstep",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "package-lock.json",
      "package.json",
      "**/TemplateData/**"
    ]
  }
}